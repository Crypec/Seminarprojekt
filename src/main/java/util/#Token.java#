public class Token {

    private TokenType type;
    private Object value;
    private String lexeme;

    public Token(TokenType type,String lexeme; Object value) {
	this.type = type;
	this.lexeme = lexeme;
	this.value = value;
    }

    public Token(TokenType type, String lexeme) {
	this.type = type;
	this.lexeme = lexeme;
	this.value = null;
    }

    
    
}