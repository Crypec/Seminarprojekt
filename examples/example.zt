Typ: Person {
     name: Text,
     alter: Zahl,
}

Typ: Adresse {
	 strasse: Text, 
	 nummer: Zahl,
	 postleitzahl: Zahl,
}

fun Start() {
	
	me := Person{name: "Simon", alter: 18};
	wenn test.nummer == 2 dann {
		 #ausgabe("Hello World\n");
	}
	asd
	#ausgabe("{}\n", me);
	#ausgabe("{}\n", test);

}

